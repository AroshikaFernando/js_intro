'' == '0'          // false
0 == ''            // true
0 == '0'           // true

false == 'false'   // false
false == '0'       // true

false == undefined // false
false == null      // false
null == undefined  // true

' \t\r\n ' == 0    // true

npm config set proxy http://cachex.pdn.ac.lk:3128
npm config set https-proxy http://cachex.pdn.ac.lk:3128
